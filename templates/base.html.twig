                  <a class="nav-link rounded me-2 text-white" href="">Décoration d'intérieur</a>
                </li> #}
              </ul>
            </div>
          </div>

          <div class="me-md-5">
            <a class="navbar-brand me-md-5" href="{{path('accueil')}}"><img src="../img/LOGO.png" class="me-md-5" style="width:150px; filter: drop-shadow(-5px 5px 2px rgba(255, 255, 255, 0.425));" alt="Logo"></a>
          </div>
          
          <div class="ms-md-5 ps-md-5">
            <a href="{{path('favorite')}}" class="me-2 text-white d-none d-md-inline"><i class="bi bi-bag-heart-fill fs-4 rounded px-1"></i></a>
            <a href="{{path('cart')}}" class="me-2 text-white d-none d-sm-inline"><i class="bi bi-basket2-fill fs-4 rounded px-1"></i></a>

             

             {% if app.user ?  'login' %}
               <a href="{{path('profile')}}" class="me-2 text-white d-none d-sm-inline"><i class="bi bi-person-circle fs-4 rounded px-1"></i></a>
             {% endif %}

            <a class="navbar-brand" href="{{app.user ? 'logout' : 'login'}}"> {{app.user ? 'Se déconnecter' : 'Se connecter'}}</a>
            
          </div>
        </div>
      </nav>
    </header>

